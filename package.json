{
  "name": "vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "echo test",
    "preview": "vite preview",
    "e2e": "concurrently -k \"npm run dev\" \"npm run wait:test\"",
    "wait:test": "wait-on http://localhost:3000/index.html && npx playwright test"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.3",
    "concurrently": "^7.6.0",
    "vite-plugin-vue-type-imports": "^0.1.3",
    "vue": "^3.2.45",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "typescript": "^4.8.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
