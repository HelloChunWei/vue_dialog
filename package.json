{
  "name": "vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "echo test",
    "preview": "vite preview",
    "e2e": "concurrently -k \"npm run dev\" \"npm run wait2\"",
    "wait2": "npm run wait",
    "wait": "wait-on http-get://127.0.0.1:5173/index.html && npm run test"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.3",
    "concurrently": "^7.6.0",
    "vite-plugin-vue-type-imports": "^0.1.3",
    "vue": "^2.7.13",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "typescript": "^4.8.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
